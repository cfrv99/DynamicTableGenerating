@model ResponseModel
@{
    ViewData["Title"] = "Home Page";
}

<a href="/Home/CreateView">Create</a>
<table class="table table-dark">
    <thead>
        <tr>
            @foreach (var header in Model.TableHeaders)
            {
                <th scope="col">@header.HeaderName</th>
            }

        </tr>
    </thead>
    <tbody>
        @foreach (var value in Model.Grouped)
        {
            <tr>
                @foreach (var item in value)
                {
                    <th scope="row">@item.Value</th>

                }
            </tr>

        }

    </tbody>
</table>
